{
    "name": "doubledog-openssh",
    "version": "1.0.0",
    "author": "John Florian",
    "summary": "A module to securely manage all things related to OpenSSH",
    "license": "GPL-3.0",
    "source": "https://www.doubledog.org/git/puppet-openssh.git",
    "project_page": "https://github.com/jflorian/doubledog-openssh",
    "issues_url": "https://github.com/jflorian/doubledog-openssh/issues",
    "operatingsystem_support": [
        {
            "operatingsystem": "CentOS",
            "operatingsystemmajrelease": "7"
        },
        {
            "operatingsystem": "Fedora",
            "operatingsystemrelease": [
                "25",
                "26",
                "27"
            ]
        }
    ],
    "dependencies": [
        {
            "name": "puppetlabs/firewall",
            "version_requirement": ">= 1.8.0 < 2.0.0"
        },
        {
            "name": "puppetlabs/stdlib",
            "version_requirement": ">= 4.0.0 < 5.0.0"
        }
    ]
}
